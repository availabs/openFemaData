select a.event_day_date, a.nri_category, a.swd_property_damage, b.swd_property_damage, b.property_damage_adjusted
from tmp_per_basis_data_swd_adjusted a
join tmp_per_basis_data_swd_non_adjusted b
on a.geoid = b.geoid
and a.event_day_date = b.event_day_date
and a.nri_category = b.nri_category
and b.property_damage_adjusted is null
